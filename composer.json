{
    "name": "zoilomora/doctrine-dbal-msaccess",
    "description": "Doctrine DBAL implementation for Microsoft Access",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-odbc": "*",
        "doctrine/dbal": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
            "ZoiloMora\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZoiloMora\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "pccomponentes/coding-standard": "^1.2",
        "phpunit/phpunit": "^9.4",
        "phpstan/phpstan": "^0.12.56"
    }
}
